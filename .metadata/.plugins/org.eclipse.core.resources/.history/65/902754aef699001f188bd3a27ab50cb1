		this.iemployee = iemployee;
	}

	@Override
	public Object invoke(Object proxy, Method method, Object[] args) throws Throwable {
		// TODO Auto-generated method stub

		
		//Pre processing 
		
		//making actual call to my service layer
		
		//post processing
	}
	

}
